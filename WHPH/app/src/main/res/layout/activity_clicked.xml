        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reps"
        android:textColor="#FFFEFE"
        android:textSize="18sp"
        app:fontFamily="@font/amarante"
        app:layout_constraintBaseline_toBaselineOf="@+id/textFirstMoveReps"
        app:layout_constraintEnd_toStartOf="@+id/textFirstMoveReps"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sets"
        android:textColor="#F3F2F2"
        android:textSize="18sp"
        app:fontFamily="@font/amarante"
        app:layout_constraintBaseline_toBaselineOf="@+id/textFirstMoveSets"
        app:layout_constraintEnd_toStartOf="@+id/textFirstMoveSets"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reps"
        android:textColor="#EDEDED"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textSecondMoveReps"
        app:layout_constraintEnd_toStartOf="@+id/textSecondMoveReps"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sets"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textSecondMoveSets"
        app:layout_constraintEnd_toStartOf="@+id/textSecondMoveSets"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reps"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textThirdMoveReps"
        app:layout_constraintEnd_toStartOf="@+id/textThirdMoveReps"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sets"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textThirdMoveSets"
        app:layout_constraintEnd_toStartOf="@+id/textThirdMoveSets"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/stopwatchClicked"
        android:layout_width="88dp"
        android:layout_height="86dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/stopw"
        android:onClick="stopwatchClicked"
        android:textColor="#E3DDDD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Timer"
        android:textColor="#F8F7F7"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textThirdMoveSets"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>